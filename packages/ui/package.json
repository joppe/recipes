{
  "name": "ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev:only": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development  postcss src/app.css -o static/app.css -w",
    "tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production  postcss src/app.css -o static/app.css",
    "dev": "concurrently \"npm run dev:only\" \"npm run tailwind:watch\"",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@recipes/eslint-config": "*",
    "@recipes/tailwind-config": "*",
    "@storybook/addon-essentials": "^7.0.0-beta.14",
    "@storybook/addon-interactions": "^7.0.0-beta.14",
    "@storybook/addon-links": "^7.0.0-beta.14",
    "@storybook/blocks": "^7.0.0-beta.14",
    "@storybook/svelte": "^7.0.0-beta.14",
    "@storybook/sveltekit": "^7.0.0-beta.14",
    "@storybook/testing-library": "^0.0.13",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.14",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "type": "module"
}
